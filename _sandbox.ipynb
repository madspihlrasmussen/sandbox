    "import matplotlib.pyplot as plt\n",
    "\n",
    "värde = 1\n",
    "faktor = 2/3\n",
    "\n",
    "summa = värde\n",
    "\n",
    "for i in range(20):\n",
    "    värde *= faktor\n",
    "    summa += värde\n",
    "\n",
    "    plt.plot(i + 1, summa, 'k.')\n",
    "\n",
    "print(f\"Summa = {summa}\")\n",
    "plt.grid('on')\n",
    "plt.show()\n",
    "\n"
   ]
  }
